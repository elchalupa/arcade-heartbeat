# arcade-heartbeat environment variables
# Copy this to .env and fill in your values
# NEVER commit .env to version control

# Your Twitch OAuth token (get from Twitch Developer Console)
# Include the "oauth:" prefix if present, or just the token
TWITCH_ACCESS_TOKEN=your_oauth_token_here

# Your Twitch channel name (lowercase, no # symbol)
TWITCH_CHANNEL=your_channel_name

# Your Twitch username (the account that owns the token)
# Usually the same as your channel name
TWITCH_USERNAME=your_username
